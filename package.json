{
  "name": "openlayers",
  "version": "3.17.1",
  "description": "Build tools and sources for developing OpenLayers based mapping applications",
  "keywords": [
    "map",
    "mapping",
    "ol3"
  ],
  "homepage": "http://openlayers.org/",
  "scripts": {
    "install": "node tasks/install.js",
    "postinstall": "closure-util update",
    "start": "node tasks/serve.js",
    "pretest": "eslint tasks test test_rendering src examples",
    "test": "node tasks/test.js",
    "debug-server": "node tasks/serve-lib.js"
  },
  "main": "dist/ol.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/openlayers/ol3.git"
  },
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/openlayers/ol3/issues"
  },
  "browser": "dist/ol.js",
  "style": [
    "css/ol.css"
  ],
  "dependencies": {
    "async": "2.0.0",
    "browserify": "13.0.1",
    "closure-util": "1.15.0",
    "derequire": "2.0.3",
    "fs-extra": "0.30.0",
    "glob": "7.0.5",
    "handlebars": "4.0.5",
    "jsdoc": "3.4.0",
    "marked": "0.3.5",
    "metalsmith": "2.1.0",
    "metalsmith-layouts": "1.6.5",
    "nomnom": "1.8.1",
    "pbf": "2.0.1",
    "pixelworks": "1.1.0",
    "rbush": "2.0.1",
    "temp": "0.8.3",
    "vector-tile": "1.2.1",
    "walk": "2.3.9"
  },
  "devDependencies": {
    "clean-css": "3.4.18",
    "coveralls": "2.11.9",
    "debounce": "^1.0.0",
    "eslint": "2.13.1",
    "eslint-config-openlayers": "5.0.0",
    "expect.js": "0.3.1",
    "gaze": "^1.0.0",
    "istanbul": "0.4.4",
    "jquery": "3.0.0",
    "mocha": "2.5.3",
    "mocha-phantomjs-core": "^1.3.0",
    "mustache": "2.2.1",
    "phantomjs-prebuilt": "2.1.7",
    "proj4": "2.3.14",
    "resemblejs": "2.2.1",
    "sinon": "1.17.4",
    "slimerjs": "0.906.2"
  },
  "eslintConfig": {
    "extends": "openlayers",
    "globals": {
      "ArrayBuffer": false,
      "Float32Array": false,
      "Uint16Array": false,
      "Uint32Array": false,
      "Uint8Array": false,
      "Uint8ClampedArray": false,
      "ol": false,
      "goog": false,
      "proj4": false
    },
    "rules": {
      "no-constant-condition": 0
    }
  },
  "ext": [
    {
      "module": "rbush",
      "browserify": true
    },
    {
      "module": "pbf",
      "browserify": true
    },
    {
      "module": "pixelworks",
      "browserify": true
    },
    {
      "module": "vector-tile",
      "name": "vectortile",
      "browserify": true
    }
  ]
}
